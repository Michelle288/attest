    "apollo-server-express": "^1.1.7",
    "bcrypt": "^1.0.3",
    "body-parser": "^1.18.2",
    "chance": "^1.0.11",
    "classnames": "^2.2.5",
    "connect": "^3.6.5",
    "cookie": "^0.3.1",
    "cors": "^2.8.4",
    "css-loader": "^0.28.9",
    "express": "^4.15.4",
    "fuse.js": "^3.2.0",
    "graphql": "^0.11.7",
    "graphql-tag": "^2.5.0",
    "http-proxy-middleware": "^0.17.4",
    "jsonwebtoken": "^8.1.0",
    "lodash": "^4.17.4",
    "moment": "^2.19.1",
    "moment-range": "^3.0.3",
    "mongoose": "^4.12.3",
    "next": "^4.2.3",
    "postcss-loader": "^2.1.0",
    "react": "^16.2.0",
    "react-animations": "^1.0.0",
    "react-apollo": "^2.0.1",
    "react-color": "^2.13.8",
    "react-datepicker": "^0.60.0",
    "react-dom": "^16.2.0",
    "react-modal": "^3.1.2",
    "react-moment": "^0.6.5",
    "react-motion": "^0.5.2",
    "react-transition-group": "^1.2.1",
    "recompose": "^0.26.0",
    "redux": "^3.7.2",
    "style-loader": "^0.20.2",
    "styled-components": "^3.1.6",
    "tachyons": "^4.9.1",
    "tachyons-components": "^1.0.0-1"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-plugin-module-resolver": "^3.1.0",
    "babel-plugin-styled-components": "^1.5.0",
    "babel-plugin-wrap-in-js": "^1.1.1",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "standard": "^10.0.3"
  },
  "standard": {
    "globals": [
      "react"
    ]
  },
  "scripts": {
    "dev": "node index.js",
    "build": "next build",
    "start-next": "NODE_ENV=production forever start index.js",
    "start-graph": "NODE_ENV=production forever start ./graphql/compiled/server.js",
    "watch-graph": "babel ./graphql/input --watch --out-dir ./graphql/compiled",
    "watch-data": "babel ./data/input --watch --out-dir ./data/compiled",
    "watch-all": "npm run dev && npm run watch-graph",
    "compile-graph": "babel ./graphql/input --out-dir ./graphql/compiled"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/grabbeh/attest.git"
  },
  "babel": {
    "presets": [
      "next/babel",
      "stage-0",
      "es2015",
      "stage-2"
    ],
    "plugins": [
      [
        "transform-es2015-modules-commonjs",
        {
          "allowTopLevelThis": true
        }
      ],
      [
        "styled-components",
        {
          "ssr": true,
          "displayName": true,
          "preprocess": false
        }
      ]
    ]
  },
  "author": "",
  "bugs": {
    "url": "https://github.com/grabbeh/attest/issues"
  },
  "homepage": "https://github.com/grabbeh/attest#readme"
}
