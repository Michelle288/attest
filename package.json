  "version": "0.0.1",
  "description": "Contract management",
  "main": "index.js",
  "dependencies": {
    "@zeit/next-css": "^0.1.2",
    "apollo-cache-inmemory": "^1.1.1",
    "apollo-client": "^2.0.3",
    "apollo-link": "^1.0.5",
    "apollo-link-context": "^1.0.2",
    "apollo-link-error": "^1.0.7",
